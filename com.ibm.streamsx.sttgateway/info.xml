<?xml version="1.0" encoding="UTF-8"?>
<toolkitInfoModel
    xmlns="http://www.ibm.com/xmlns/prod/streams/spl/toolkitInfo"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xmlns:common="http://www.ibm.com/xmlns/prod/streams/spl/common"
    xsi:schemaLocation="http://www.ibm.com/xmlns/prod/streams/spl/toolkitInfo toolkitInfoModel.xsd">
  <identity>
    <name>com.ibm.streamsx.sttgateway</name>
    <description>
    This toolkit does Speech To Text transcription using an external provider such as the IBM Watson STT cloud service 
    or on the IBM Cloud Pak for Data (CP4D i.e. private cloud).
    
    **Note:** This toolkit is **not** supported on Red Hat Enterprise Linux Workstation release **6.x**
    
    **Note:** This toolkit requires c++11 support.
    </description>
    <version>2.3.0</version>
    <requiredProductVersion>4.2.1.6</requiredProductVersion>
  </identity>
  <dependencies>
    <toolkit>
      <common:name>com.ibm.streamsx.json</common:name>
      <common:version>[1.4.6,7.0.0]</common:version>
    </toolkit>
    <toolkit>
      <common:name>com.ibm.streamsx.inet</common:name>
      <common:version>[2.3.6,7.0.0]</common:version>
    </toolkit>
  </dependencies>
  <resources>
    <messageSet name="SttGatewayResource">
      <lang default="true">en_US/SttGatewayResource.xlf</lang>
      <lang>de_DE/SttGatewayResource.xlf</lang>
      <lang>es_ES/SttGatewayResource.xlf</lang>
      <lang>fr_FR/SttGatewayResource.xlf</lang>
      <lang>it_IT/SttGatewayResource.xlf</lang>
      <lang>ja_JP/SttGatewayResource.xlf</lang>
      <lang>pt_BR/SttGatewayResource.xlf</lang>
      <lang>zh_CN/SttGatewayResource.xlf</lang>
      <lang>zh_TW/SttGatewayResource.xlf</lang>
    </messageSet>
  </resources>
</toolkitInfoModel>
